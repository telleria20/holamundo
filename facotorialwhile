// factorial.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main()

{
    int n,i,p=1;
	
	do {
	cout<< "ingrese un numero: "<<endl;
	cin>>n;
	} while (n<0);
	i=1;
	while (i<=n)
	{ 
		p=p*i;
	    i++;
	}
	cout<<"El producto es: "<<p<<endl;


	getch();
}

