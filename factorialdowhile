// factorial.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main()

{
    int n,i,p;
	p=1;
	
	do {
	cout<< "ingrese un numero: \n";
	cin>>n;
	} while (n<0);

	for (i=1; i<=n; i++)
		p=p*i;
	cout<<"El producto es \n"<<p<<endl;


	getch();
}
